{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "app/src/*"
      ],
      "@assets/*": [
        "app/src/assets/*"
      ],
      "@components/*": [
        "app/src/components/*"
      ],
      "@constants/*": [
        "app/src/constants/*"
      ],
      "@hooks/*": [
        "app/src/hooks/*"
      ],
      "@pages/*": [
        "app/src/pages/*"
      ],
      "@router/*": [
        "app/src/router/*"
      ],
      "@runtime/*": [
        "app/src/runtime/*"
      ],
      "@scss/*": [
        "app/src/scss/*"
      ],
      "@shared/*": [
        "app/src/shared/*"
      ],
      "@store/*": [
        "app/src/store/*"
      ],
      "@types/*": [
        "app/src/types/*"
      ],
      "@utils/*": [
        "app/src/utils/*"
      ],
      "@customize/*": [
        "customize/*"
      ]
    },
    "types": [
      "node",
      "vite/client",
      "vite-plugin-svgr/client"
    ]
  },
  "include": [
    "app/src",
    "customize",
    "scripts"
  ]
}